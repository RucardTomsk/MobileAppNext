<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="4dip" android:color="#01256b" />
            <gradient android:angle="180" android:startColor="#002163" android:endColor="#1f499c" /> </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="4dip" android:color="#01256b" />
            <gradient android:angle="180" android:startColor="#002163" android:endColor="#1f499c" /></shape>
    </item>
    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="4dip" android:color="#01256b" />
            <gradient android:angle="180" android:startColor="#002163" android:endColor="#1f499c" />
        </shape>
    </item>
</selector>